{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "useBuiltIns": "entry"
      }
    ],
    [
      "@babel/preset-flow"
    ],
    [
      "@babel/preset-typescript"
    ]
  ],
  "plugins": [
    "@babel/proposal-class-properties"
  ]
}
